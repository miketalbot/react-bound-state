{
  "name": "react-bound-state",
  "version": "1.0.38",
  "description": "Binding library for React",
  "author": "miketalbot",
  "license": "MIT",
  "repository": "miketalbot/react-bound-state",
  "main": "dist/index.js",
  "typings": "dist/types.d.ts",
  "types": "dist/types.d.ts",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "node ./node_modules/versiony-cli/bin/cli.js ./package.json --patch && run-s build && cp ./src/types.d.ts ./dist",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cp -r ./docs ./pages && cd example && npm install && npm run build && cp -r ./build ../pages/example",
    "deploy": "gh-pages -d pages",
    "document": "rm -rf ./docs && jsdoc -c ./doc.conf.json",
    "tsd": "jsdoc -t node_modules/tsd-jsdoc/dist -r ./src/lib/observable-state.js"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "better-docs": "^2.0.1",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "docdash": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "jsdoc": "^3.6.4",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "prettier": "^2.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "tsd-jsdoc": "^2.5.0",
    "versiony-cli": "^1.3.0"
  },
  "files": [
    "dist",
    "docs"
  ]
}